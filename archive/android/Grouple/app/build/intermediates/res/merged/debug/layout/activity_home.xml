<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/homeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/background_color"
    android:padding="5dp" >

  	<ImageView
		android:id="@+id/homeLogoImageViewHA"
	    android:layout_width="match_parent"
	    android:layout_height="fill_parent"
	    android:layout_marginRight="20dp"
	    android:layout_above="@+id/navLayout"
	    android:layout_marginLeft="20dp"
	    android:layout_marginTop="5dp"
	    android:contentDescription="test"
	    android:gravity="center_vertical"
	    android:layout_gravity="center_vertical"
	    android:src="@drawable/logo_grouple"
	    android:visibility="visible" />
    
  	<LinearLayout
  	    android:id="@+id/navLayout"
  	    android:orientation="vertical"
  	   	android:layout_alignParentBottom="true"
  	    android:layout_marginBottom="5dp"
  	    android:layout_width="match_parent"
  	    android:layout_height="wrap_content" >
		<LinearLayout
	  	    android:id="@+id/topRowNavLayoutHA"
	  	    android:orientation="horizontal"
	  	    android:layout_marginBottom="5dp"
	  	    android:layout_width="match_parent"
	  	    android:layout_height="wrap_content" >
	      	    
	      	    
	 	    <Button
	 	        android:layout_height="wrap_content"
	  	        android:layout_width="wrap_content"
	 	        android:id="@+id/userButtonHA"
	 	        android:layout_margin="5dp"
	 	        android:layout_weight="1"
	 	        android:background="@drawable/selector_userbutton"
	 	        android:textStyle="bold"
	 	        android:adjustViewBounds="true"
	 	        android:fontFamily="sans-serif"
	 	        android:paddingBottom="10dp"
	 	        android:gravity="bottom|center_horizontal"
	 	        android:onClick="navigate"
	 	        android:text="Profile"
	 	        android:textColor="@color/text_color"
	 	        android:textSize="16sp" />
	 	    
	 		<Button
	 		   	android:layout_height="wrap_content"
	  	        android:layout_width="wrap_content"
	  	        android:layout_margin="5dp"
	 	        android:id="@+id/messagesButtonHA"		
	 	        android:paddingBottom="10dp"
	 	        android:background="@drawable/selector_messagesbutton"
	 	        android:textStyle="bold"
	 	        android:layout_weight="1"
	 	       	android:fontFamily="sans-serif"
	 	        android:gravity="bottom|center_horizontal"
	 	        android:onClick="navigate"
	 	        android:text="Messages"
	 	        android:textColor="@color/text_color"
	 	        android:textSize="16sp" />
	
	 	    <Button
	 	      	android:layout_height="wrap_content"
	  	        android:layout_width="wrap_content"
	 	        android:id="@+id/settingsButtonHA"
				android:layout_margin="5dp"
	 	        android:layout_weight="1"
	 	       	android:paddingBottom="10dp"
	 	        android:background="@drawable/selector_settingsbutton"
	 	        android:textStyle="bold"
	 	     	android:fontFamily="sans-serif"
	 	        android:gravity="bottom|center_horizontal"
	 	        android:onClick="navigate"
	 	        android:text="Settings"
	 	        android:textColor="@color/text_color"
	 	        android:textSize="16sp" />
	
	 	</LinearLayout>
	      	
	      	
	   	<LinearLayout
	   	    android:id="@+id/bottRowNavLayoutHA"
	   	    android:orientation="horizontal"
	   	    android:layout_marginTop="5dp"
	   	    android:layout_width="match_parent"
	   	    android:layout_height="wrap_content" >
	    	
	    	<Button
	    	   	android:layout_height="wrap_content"
	  	        android:layout_width="wrap_content"
	  	        android:layout_margin="5dp"
	   	        android:id="@+id/friendsButtonHA"
	   	        android:layout_weight="1"
	   	        android:background="@drawable/selector_friendsbutton"
	   	        android:paddingBottom="10dp"
	   	        android:gravity="bottom|center_horizontal"
	   	        android:onClick="navigate"
	   	        android:text="Friends"
	   	        android:textColor="@color/text_color"
	   	        android:textStyle="bold"
	   	       	android:fontFamily="sans-serif"
	   	        android:textSize="16sp" />
	
	
	  	    
	  	    <Button
	  	       	android:layout_height="wrap_content"
	  	        android:layout_width="wrap_content"
	  	        android:id="@+id/groupsButtonHA"
	  	        android:layout_weight="1"
	  	        android:layout_margin="5dp"
	  	        android:background="@drawable/selector_groupsbutton"
	  	        android:textStyle="bold"
	  	        android:fontFamily="sans-serif"
	  	        android:gravity="bottom|center_horizontal"
	  	        android:onClick="navigate"
	  	     	android:paddingBottom="10dp"
	  	        android:text="Groups"
	  	        android:textColor="@color/text_color"
	  	        android:textSize="16sp" />
	
	 		<Button
	  	        android:id="@+id/eventsButtonHA"
	  	        android:layout_weight="1"
	  	        android:layout_height="wrap_content"
	  	        android:layout_width="wrap_content"
	  	        android:background="@drawable/selector_eventsbutton"
	      	    android:textStyle="bold"
	      	   	android:layout_margin="5dp"
	  	        android:fontFamily="sans-serif"
	  	        android:gravity="bottom|center_horizontal"
	  	        android:onClick="navigate"
	  	        android:text="Events"
	  	        android:textColor="@color/text_color"
	  	        android:paddingBottom="10dp"
	  	        android:textSize="16sp" />
	
		</LinearLayout>
	</LinearLayout>
</RelativeLayout><!-- From: file:/Users/brett/Documents/Mierzelancing/Workspaces/grouple-workspace/Grouple/app/src/main/res/layout/activity_home.xml -->